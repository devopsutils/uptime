{
  "name": "@khalidx/uptime-app",
  "private": true,
  "version": "0.1.0",
  "description": "An uptime monitoring serverless application that is easy to understand, use, and extend.",
  "author": "Khalid Zoabi <kzoabi@outlook.com>",
  "keywords": [
    "uptime",
    "monitoring",
    "serverless"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "serverless offline start",
    "deploy": "serverless deploy -v",
    "deploy-function": "serverless deploy --function $FUNCTION",
    "logs-function": "serverless logs --function $FUNCTION --tail",
    "undeploy": "serverless remove"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/hapi__joi": "^15.0.2",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
  }
}
